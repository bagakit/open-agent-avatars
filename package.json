{
  "name": "@bagakit/open-agent-avatars",
  "version": "0.1.1",
  "description": "A ready-to-use set of open-agent avatar SVGs for quickly bootstrapping other projects.",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.cjs"
    },
    "./index.json": "./index.json",
    "./20260202": {
      "types": "./20260202/index.d.ts",
      "import": "./20260202/index.mjs",
      "require": "./20260202/index.cjs"
    },
    "./20260202/*": {
      "types": "./20260202/mod/*.d.ts",
      "import": "./20260202/mod/*.mjs",
      "require": "./20260202/mod/*.cjs"
    }
  },
  "scripts": {
    "generate": "node ./scripts/generate-index.mjs",
    "prepack": "node ./scripts/generate-index.mjs",
    "publish:public": "npm publish --access public"
  },
  "publishConfig": { 
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
